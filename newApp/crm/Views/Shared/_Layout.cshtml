<!DOCTYPE html>
<html lang="en">
<head>
<partial></partial>
<title>@ViewData["Title"] - CRM NewApp-Admin</title>
<!-- Editable CSS -->
<link rel="stylesheet" type="text/css" href="~/css/dataTables.bootstrap4.css">
<link rel="stylesheet" type="text/css" href="~/css/responsive.dataTables.min.css">
<!-- Custom CSS -->
<link href="~/css/style.min.css" rel="stylesheet">
<!-- page css -->
<link href="~/css/pages/inbox.css" rel="stylesheet">
<link href="~/css/ribbon-page.css" rel="stylesheet">

<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<link rel="stylesheet" href="~/crm.styles.css" asp-append-version="true" />
</head>

<body class="skin-blue fixed-layout">
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">Elite admin</p>
        </div>
    </div>

    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        @Html.Partial("_Header")
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->

        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        @Html.Partial("_LeftSideBar")
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->

        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Bread crumb and right sidebar toggle -->
                <!-- ============================================================== -->
                @Html.Partial("_PageTitle")
                <!-- ============================================================== -->
                <!-- End Bread crumb and right sidebar toggle -->
                <!-- ============================================================== -->

                <div class="container">
                    <main role="main" class="pb-3">
                        @RenderBody()
                    </main>
                </div>

                <!-- ============================================================== -->
                <!-- Right sidebar -->
                <!-- ============================================================== -->
                @Html.Partial("_RightSideBar")
                <!-- ============================================================== -->
                <!-- End Right sidebar -->
                <!-- ============================================================== -->
            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- End Page wrapper  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- footer -->
        <!-- ============================================================== -->
        <div th:insert="~{/general/footer.html}"></div>
        <!-- ============================================================== -->
        <!-- End footer -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- End Wrapper -->
    <!-- ============================================================== -->
    <footer class="footer">
        © 2025 CRM by TahinaBemax</a>
    </footer>

    <script src="~/js/library/jquery-3.2.1.min.js" type="text/javascript"></script>
    <!--    &lt;!&ndash; Bootstrap tether Core JavaScript &ndash;&gt;-->
    <script src="~/js/library/popper.min.js" type="text/javascript"></script>
    <script src="~/js/library/bootstrap.min.js" type="text/javascript"></script>
    <!--    &lt;!&ndash; slimscrollbar scrollbar JavaScript &ndash;&gt;-->
    <script src="~/js/library/perfect-scrollbar.jquery.min.js" type="text/javascript">></script>
    <!--Wave Effects -->
    <script src="~/js/library/waves.js" type="text/javascript"></script>
    <!--Menu sidebar -->
    <script src="~/js/library/sidebarmenu.js" type="text/javascript"></script>
    <!--stickey kit -->
    <script src="/js/library/sticky-kit.min.js"></script>
    <script src="/js/library/jquery.sparkline.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
